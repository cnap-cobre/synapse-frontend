{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "extends": [
      "eslint:all",
      "plugin:react/all"
    ],
    "sort-imports": ["error", {
      "memberSyntaxSortOrder": ["all", "single", "multiple", "none"],
      "ignoreCase": true
    }]
  }
}