{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "extends": [
    "airbnb",
    "plugin:flowtype/recommended"
  ],
  "rules": {
    "react/jsx-filename-extension": "off",
    "import/prefer-default-export": "off",
    "no-continue": "off",
    "no-restricted-syntax": "warn",
    "react/prop-types": "off"
  },
  "env": {
    "browser": true
  },
  "plugins": [
    "flowtype"
  ],
  "globals": {
    "expect": true
  }
}